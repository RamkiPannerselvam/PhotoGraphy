{
  "name": "Simple",
  "version": "1.0.0",
  "main": "dist/index.js",
  "dependencies": {
    "css-loader": "^0.28.11",
    "debounce": "^1.0.2",
    "element-closest": "^2.0.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-dropdown": "^1.5.0",
    "react-router-dom": "^4.3.1",
    "react-table": "^6.8.6",
    "style-loader": "^0.21.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^4.0.0",
    "envify": "^1.2.1",
    "envify-loader": "^0.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "expect": "^1.20.2",
    "gaze-cli": "^0.2.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "mocha-bamboo-reporter": "^1.1.1",
    "node-sass": "^4.9.0",
    "node-sass-import": "^1.1.1",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.2.0",
    "sass-lint": "^1.10.2",
    "vulcanize": "^1.15.4",
    "webpack": "^4.16.2",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.5"
  },
  "scripts": {
    "it": "webpack-dev-server --hot",
    "start": "npm run test:watch",
    "test": "mocha -R mocha-bamboo-reporter",
    "test:watch": "gaze 'npm test' 'test/**/*.js'",
    "testCoverage": "cross-env NODE_ENV=test nyc --reporter=lcov npm test",
    "build": "npm-run-all lint build:js build:css",
    "build:example": "npm-run-all build:js:example build:css:example",
    "build:js": "cross-env NODE_ENV=development babel src --out-dir dist",
    "buildDev:js": "cross-env NODE_ENV=development babel --watch src --out-dir dist",
    "build:js:example": "cross-env NODE_ENV=development webpack --progress --color",
    "buildDev:js:example": "cross-env NODE_ENV=development webpack --watch --progress --color",
    "build:css": "node-sass src/index.scss dist/index.build.css --importer node_modules/node-sass-import",
    "build:css:example": "node-sass example/index.scss example/index.build.css --importer node_modules/node-sass-import",
    "buildDev:css:example": "node-sass example/index.scss example/index.build.css --watch --importer node_modules/node-sass-import",
    "build:html:example": "vulcanize --inline-scripts --inline-css example/index.html > example/index.min.html"
  },
  "abcostandard": {
    "ignore": [
      "/dist/",
      "/example/"
    ]
  }
}
